{
  "name": "t-digital-website",
  "version": "1.0.0",
  "description": "Professional IT company website with admin panel and multilingual support",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm run build-css && npm run build-js",
    "build-css": "postcss css/style.css -o css/style.min.css",
    "build-js": "terser js/main.js -o js/main.min.js"
  },
  "keywords": ["website", "it-company", "admin-panel", "multilingual"],
  "author": "T.Digital",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "bcryptjs": "^2.4.3",
    "multer": "^1.4.5-lts.1",
    "dotenv": "^16.3.1",
    "axios": "^1.6.0",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "express-validator": "^7.0.1",
    "csurf": "^1.11.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "terser": "^5.24.0",
    "postcss": "^8.4.31",
    "autoprefixer": "^10.4.16",
    "cssnano": "^6.0.1"
